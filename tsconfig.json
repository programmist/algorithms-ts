{
  "compilerOptions": {
    "target": "es2022",
    "module": "nodenext",
    // "rootDir": "./src",
    "moduleResolution": "nodenext",
    "sourceMap": true,
    // "outDir": "./js",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
